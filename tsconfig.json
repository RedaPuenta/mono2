{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strict": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ESNext",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "paths": {
      "@lib/configs": ["libs/configs"],
      "@lib/configs/*": ["libs/configs/*"],
      "@lib/filters": ["libs/filters"],
      "@lib/filters/*": ["libs/filters/*"],
      "@lib/guards": ["libs/guards"],
      "@lib/guards/*": ["libs/guards/*"],
      "@lib/interceptors": ["libs/interceptors"],
      "@lib/interceptors/*": ["libs/interceptors/*"],
      "@lib/modules": ["libs/modules"],
      "@lib/modules/*": ["libs/modules/*"],
      "@lib/pipes": ["libs/pipes"],
      "@lib/pipes/*": ["libs/pipes/*"],
      "@lib/strategys": ["libs/strategys"],
      "@lib/strategys/*": ["libs/strategys/*"],
      "@lib/decorators": ["libs/decorators"],
      "@lib/decorators/*": ["libs/decorators/*"],
      "@lib/registrys": ["libs/registrys"],
      "@lib/registrys/*": ["libs/registrys/*"],
      "@lib/data": ["libs/data"],
      "@lib/data/*": ["libs/data/*"],
      "@lib/scalars": ["libs/scalars"],
      "@lib/scalars/*": ["libs/scalars/*"]
    }
  }
}
