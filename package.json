  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/configs(|/.*)$": "<rootDir>/libs/configs/src/$1",
      "^@lib/filters(|/.*)$": "<rootDir>/libs/filters/src/$1",
      "^@lib/guards(|/.*)$": "<rootDir>/libs/guards/src/$1",
      "^@lib/interceptors(|/.*)$": "<rootDir>/libs/interceptors/src/$1",
      "^@lib/modules(|/.*)$": "<rootDir>/libs/modules/src/$1",
      "^@lib/pipes(|/.*)$": "<rootDir>/libs/pipes/src/$1",
      "^@lib/strategys(|/.*)$": "<rootDir>/libs/strategys/src/$1",
      "^@lib/decorators(|/.*)$": "<rootDir>/libs/decorators/src/$1",
      "^@lib/registrys(|/.*)$": "<rootDir>/libs/registrys/src/$1",
      "^@lib/data(|/.*)$": "<rootDir>/libs/data/src/$1",
      "^@lib/scalars(|/.*)$": "<rootDir>/libs/scalars/src/$1"
    }
  }
